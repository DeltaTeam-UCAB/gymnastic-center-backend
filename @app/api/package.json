{
  "name": "@mono/api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "tscc",
    "start": "tscc && node dist/main",
    "start:dev": "tscc -w",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,libs,test}/**/*.ts\" --fix",
    "test:e2e": "tscc --project ./tsconfig.test.json && node --experimental-vm-modules node_modules/jest/bin/jest.js --config ./dist_test/tests/e2e/core/impl/jest/runner/jest-e2e.js",
    "test:unit": "tscc --project ./tsconfig.test.json && node --test ./dist_test/tests/unit/core/impl/node-testing/runner/setup.js",
    "test:unit2": "tscc --project ./tsconfig.test.json && node --experimental-vm-modules node_modules/jest/bin/jest.js --config ./dist_test/tests/unit/core/impl/jest/runner/jest-unit.js",
    "test:unit3": "tscc --project ./tsconfig.test.json && mocha --recursive ./dist_test/tests/unit/suits/**/suit.tests.js",
    "test:unit4": "tscc --project ./tsconfig.test.json && pnpx jasmine --config=./jasmine.json",
    "test:unit5": "tscc --project ./tsconfig.test.json && pnpx ava",
    "test:unit6": "tscc --project ./tsconfig.test.json && tap run --disable-coverage --reporter=spec ./dist_test/tests/unit/suits/**/suit.tests.js",
    "test:unit7": "tscc --project ./tsconfig.test.json && tape ./dist_test/tests/unit/suits/**/suit.tests.js",
    "test:unit8": "tscc --project ./tsconfig.test.json && vitest run --config ./dist_test/tests/unit/core/impl/vitest/runner/vitest.js",
    "test:unit9": "tscc --project ./tsconfig.test.json && node ./dist_test/tests/unit/core/impl/japa/runner/setup.js",
    "test:unit10": "tscc --project ./tsconfig.test.json && uvu ./dist_test/tests/unit/suits suit.tests.js$",
    "test:unit11": "tscc --project ./tsconfig.test.json && ./node_modules/.bin/ars ./dist_test/tests/unit/suits/**/suit.tests.js",
    "dependencies-graph": "npx depcruise src --include-only \"^src\" --config --output-type dot | dot -T svg > dependency-graph.svg"
  },
  "imports": {
    "#corejest": "./dist_test/tests/unit/core/impl/jest/index.js",
    "#corevitest": "./dist_test/tests/unit/core/impl/vitest/index.js",
    "#coreava": "./dist_test/tests/unit/core/impl/ava/index.js",
    "#coreuvu": "./dist_test/tests/unit/core/impl/uvu/index.js",
    "#corejapa": "./dist_test/tests/unit/core/impl/japa/index.js",
    "#coretap": "./dist_test/tests/unit/core/impl/tap/index.js",
    "#coretape": "./dist_test/tests/unit/core/impl/tape/index.js",
    "#coreraisins": "./dist_test/tests/unit/core/impl/raisins/index.js",
    "#corejasmine": "./dist_test/tests/unit/core/impl/jasmine/index.js",
    "#coremocha": "./dist_test/tests/unit/core/impl/mocha/index.js",
    "#corenode": "./dist_test/tests/unit/core/impl/node-testing/index.js"
  },
  "dependencies": {
    "@mono/array-methods": "workspace:*",
    "@mono/number-methods": "workspace:*",
    "@mono/object-utils": "workspace:^0.0.1",
    "@mono/pattern-matching": "workspace:*",
    "@mono/promise-methods": "workspace:*",
    "@mono/string-methods": "workspace:*",
    "@mono/types-utils": "workspace:*",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "amqplib": "^0.10.4",
    "async-limiter": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.2.0",
    "dotenv": "^16.4.5",
    "glob": "^10.3.10",
    "js-sha256": "^0.9.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@japa/expect": "^3.0.2",
    "@japa/runner": "^3.1.4",
    "@jest/globals": "^29.7.0",
    "@mono/compiler": "workspace: *",
    "@mono/eslint-config": "workspace:^1.0.0",
    "@mono/test-utils": "workspace: *",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.2.1",
    "@types/amqplib": "^0.10.5",
    "@types/express": "^4.17.13",
    "@types/glob": "^8.0.1",
    "@types/jasmine": "^5.1.4",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.7",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "assert-raisins": "^2.0.1",
    "ava": "^6.1.3",
    "jasmine": "^5.1.0",
    "jest": "29.7.0",
    "mocha": "^10.4.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tap": "^19.0.2",
    "tape": "^5.7.5",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "uvu": "^0.5.6",
    "vitest": "^0.34.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "ava": {
    "files": [
      "./dist_test/tests/unit/suits/**/suit.tests.js"
    ],
    "require": [
      "./dist_test/tests/unit/core/impl/ava/runner/setup.js"
    ],
    "failWithoutAssertions": false,
    "verbose": true
  }
}
