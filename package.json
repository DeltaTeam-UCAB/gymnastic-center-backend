{
  "name": "gymnastic-center",
  "version": "1.0.0",
  "description": "Gymnastic center API build with pnpm, typescript and nest",
  "type": "module",
  "scripts": {
    "start:dev": "pnpm --recursive --parallel --stream run start:dev",
    "build": "pnpm --recursive --parallel --stream run build",
    "start": "pnpm --recursive --parallel --stream run start",
    "start:prod": "pnpm --recursive --parallel --stream run start:prod",
    "lint": "pnpm --recursive --parallel --stream run lint",
    "commit": "sui-mono commit",
    "format": "prettier --write \"**/**/src/**/*.{ts,tsx}\" \"**/**/lib/**/*.{ts,tsx}\" \"**/**/test/**/*.ts\"",
    "prepare-commithooks": "husky install",
    "test:unit": "pnpm --recursive --parallel --stream run test:unit",
    "test:e2e": "pnpm --recursive --parallel --stream run test:e2e",
    "postinstall": "pnpm run prepare-commithooks",
    "dependencies-graph": "pnpm --recursive --parallel --stream run dependencies-graph"
  },
  "keywords": [],
  "author": "danixl30",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@japa/expect": "^3.0.2",
    "@japa/runner": "^3.1.4",
    "@jest/globals": "^29.7.0",
    "@s-ui/mono": "^2.34.0",
    "@types/amqplib": "^0.10.5",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.8.8",
    "@types/nodemailer": "^6.4.15",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "assert-raisins": "^2.0.1",
    "ava": "^6.1.3",
    "concurrently": "^8.2.2",
    "dependency-cruiser": "^12.6.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphviz": "^0.0.9",
    "husky": "^8.0.3",
    "jasmine": "^5.1.0",
    "mocha": "^10.4.0",
    "prettier": "^2.3.2",
    "tap": "^19.0.2",
    "tape": "^5.7.5",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.3.3",
    "uvu": "^0.5.6",
    "vitest": "^0.34.6"
  }
}